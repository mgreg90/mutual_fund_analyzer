<p id="notice"><%= notice %></p>

<h1>Listing Mutual Fund Lists</h1>

<table>
  <thead>
    <tr>
      <th>Ticker list</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mutual_fund_lists.each do |mutual_fund_list| %>
      <tr>
        <td><%= mutual_fund_list.ticker_list %></td>
        <td><%= link_to 'Show', mutual_fund_list %></td>
        <td><%= link_to 'Edit', edit_mutual_fund_list_path(mutual_fund_list) %></td>
        <td><%= link_to 'Destroy', mutual_fund_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mutual fund list', new_mutual_fund_list_path %>
