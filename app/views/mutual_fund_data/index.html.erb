<p id="notice"><%= notice %></p>

<h1>Listing Mutual Fund Data</h1>

<table>
  <thead>
    <tr>
      <th>Raw yf date</th>
      <th>Raw mstar data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mutual_fund_data.each do |mutual_fund_datum| %>
      <tr>
        <td><%= mutual_fund_datum.raw_yf_date %></td>
        <td><%= mutual_fund_datum.raw_mstar_data %></td>
        <td><%= link_to 'Show', mutual_fund_datum %></td>
        <td><%= link_to 'Edit', edit_mutual_fund_datum_path(mutual_fund_datum) %></td>
        <td><%= link_to 'Destroy', mutual_fund_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mutual fund datum', new_mutual_fund_datum_path %>
